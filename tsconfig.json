{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./*"
            ],
            "@/components/*": [
                "./components/*"
            ],
            "@/modules/*": [
                "./modules/*"
            ],
            "@/hooks/*": [
                "./hooks/*"
            ],
            "@/store/*": [
                "./store/*"
            ],
            "@/services/*": [
                "./services/*"
            ],
            "@/utils/*": [
                "./utils/*"
            ]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts"
    ]
}